.container{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

scroll-view{
  text-align: center;
  width: 750rpx;
  height: 100vh;
}

.img_wrap{
  width: 700rpx;
  height: 350rpx;
  display: flex;
  overflow: hidden;
  background: red;
  margin-bottom: 20rpx;
  margin: 20rpx auto;
  border-radius: 10rpx;
}

.img_wrap image{
  width: 100%;
  height: 100%;
}